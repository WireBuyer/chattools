{
    "compilerOptions": {
        "checkJs": true,
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2020",
        "jsx": "react-jsx",
        "strict": false,
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "resolveJsonModule": true,
        "lib": ["ESNext", "DOM"],
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}